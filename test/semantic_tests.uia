int x[3];
int main(int arg) {
    int y = 3;

    // Index out of bounds
    int c = x[1];
    int d = x[y];

    // Divide by zero
    int t = addInt(1, 4/0);

    // Assignment type mismatch
    char c = 10;
    bool test_bool = 'a';

    // Operator type mismatch
    int op = true + 1;

    // Function argument type mismatch
    int b = addInt(1, 't');

    // Function argument count mismatch
    int a = addInt(1, 2, 3);

    char z = 'c';

    return x[z];
}

int addInt(int x, int y) {
    return x + y;
}

// Return type mismatch
bool testReturnType() {
    return 'h';
}